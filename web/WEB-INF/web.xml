<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns="http://xmlns.jcp.org/xml/ns/javaee"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_4_0.xsd"
         version="4.0">

    <servlet>
        <servlet-name>LoginServlet</servlet-name>
        <servlet-class>Servlet.LoginServlet</servlet-class>
    </servlet>

    <servlet-mapping>
        <servlet-name>LoginServlet</servlet-name>
        <url-pattern>/Servlet.LoginServlet.do</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>RegisterServlet</servlet-name>
        <servlet-class>Servlet.RegisterServlet</servlet-class>
    </servlet>

    <servlet-mapping>
        <servlet-name>RegisterServlet</servlet-name>
        <url-pattern>/Servlet.RegisterServlet.do</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>OrderServlet</servlet-name>
        <servlet-class>Servlet.OrderServlet</servlet-class>
    </servlet>

    <servlet-mapping>
        <servlet-name>OrderServlet</servlet-name>
        <url-pattern>/Servlet.OrderServlet.do</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>DishServlet</servlet-name>
        <servlet-class>Servlet.DishServlet</servlet-class>
    </servlet>

    <servlet-mapping>
        <servlet-name>DishServlet</servlet-name>
        <url-pattern>/Servlet.DishServlet.do</url-pattern>
    </servlet-mapping>


    <servlet>
        <servlet-name>RestaurantServlet</servlet-name>
        <servlet-class>Servlet.RestaurantServlet</servlet-class>
    </servlet>

    <servlet-mapping>
        <servlet-name>RestaurantServlet</servlet-name>
        <url-pattern>/Servlet.RestaurantServlet.do</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>EventServlet</servlet-name>
        <servlet-class>Servlet.EventServlet</servlet-class>
    </servlet>

    <servlet-mapping>
        <servlet-name>EventServlet</servlet-name>
        <url-pattern>/Servlet.EventServlet.do</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>BadgeServlet</servlet-name>
        <servlet-class>Servlet.BadgeServlet</servlet-class>
    </servlet>

    <servlet-mapping>
        <servlet-name>BadgeServlet</servlet-name>
        <url-pattern>/Servlet.BadgeServlet.do</url-pattern>
    </servlet-mapping>

</web-app>